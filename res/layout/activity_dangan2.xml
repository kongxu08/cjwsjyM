<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/re_outofoffice_index"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#FFFFFF">
   
   <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:orientation="vertical"> 
   	
   		<include layout="@layout/top_layout" />  
      
       	<RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="140dp" 
            android:background="@drawable/dangan1" />
    
	    <TextView
	            android:id="@+id/tv_sousuo"
	            android:layout_width="300dp"
	            android:layout_height="35dp"
	            android:background="@drawable/textcorner6"
	            android:textSize="16sp"
	            android:textColor="#FFFFFF"
	            android:gravity="center"
	         	android:layout_gravity="center"
	         	android:layout_marginTop="20dp"
	            android:text="综合搜索" />
	    <TextView
	            android:id="@+id/tv_shoucang"
	            android:layout_width="300dp"
	            android:layout_height="35dp"
	            android:background="@drawable/textcorner6"
	            android:textSize="16sp"
	            android:textColor="#FFFFFF"
	            android:gravity="center"
	         	android:layout_gravity="center"
	         	android:layout_marginTop="10dp"
	            android:text="收藏夹" />
	    <TextView
	            android:id="@+id/tv_jieyue"
	            android:layout_width="300dp"
	            android:layout_height="35dp"
	            android:background="@drawable/textcorner6"
	            android:textSize="16sp"
	            android:textColor="#FFFFFF"
	            android:gravity="center"
	         	android:layout_gravity="center"
	         	android:layout_marginTop="10dp"
	            android:text="借阅申请单(0条)" />
	   <TextView
		   android:id="@+id/tv_xiazai"
		   android:layout_width="300dp"
		   android:layout_height="35dp"
		   android:background="@drawable/textcorner6"
		   android:textSize="16sp"
		   android:textColor="#FFFFFF"
		   android:gravity="center"
		   android:layout_gravity="center"
		   android:layout_marginTop="10dp"
		   android:text="下载申请单(0条)" />
	   <TextView
		   android:id="@+id/tv_daiban"
		   android:layout_width="300dp"
		   android:layout_height="35dp"
		   android:background="@drawable/textcorner6"
		   android:textSize="16sp"
		   android:textColor="#FFFFFF"
		   android:gravity="center"
		   android:layout_gravity="center"
		   android:layout_marginTop="10dp"
		   android:text="我的待办(0条)" />
	   <TextView
		   android:id="@+id/tv_dangan"
		   android:layout_width="300dp"
		   android:layout_height="35dp"
		   android:background="@drawable/textcorner6"
		   android:textSize="16sp"
		   android:textColor="#FFFFFF"
		   android:gravity="center"
		   android:layout_gravity="center"
		   android:layout_marginTop="10dp"
		   android:text="电子档案下载" />
	   <TextView
		   android:id="@+id/tv_faqi"
		   android:layout_width="300dp"
		   android:layout_height="35dp"
		   android:background="@drawable/textcorner6"
		   android:textSize="16sp"
		   android:textColor="#FFFFFF"
		   android:gravity="center"
		   android:layout_gravity="center"
		   android:layout_marginTop="10dp"
		   android:text="我的发起" />
	   <TextView
		   android:id="@+id/tv_shenpi"
		   android:layout_width="300dp"
		   android:layout_height="35dp"
		   android:background="@drawable/textcorner6"
		   android:textSize="16sp"
		   android:textColor="#FFFFFF"
		   android:gravity="center"
		   android:layout_gravity="center"
		   android:layout_marginTop="10dp"
		   android:text="我的审批" />
	   <TextView
		   android:id="@+id/tv_zaixian"
		   android:layout_width="300dp"
		   android:layout_height="35dp"
		   android:background="@drawable/textcorner6"
		   android:textSize="16sp"
		   android:textColor="#FFFFFF"
		   android:gravity="center"
		   android:layout_gravity="center"
		   android:layout_marginTop="10dp"
		   android:text="在线查看" />

	</LinearLayout>
</RelativeLayout>
